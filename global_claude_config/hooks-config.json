{
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "",
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/pre_tool_use.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type PreToolUse --summarize" }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "",
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/post_tool_use.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type PostToolUse --summarize" }
        ]
      }
    ],
    "PostToolUseFailure": [
      {
        "matcher": "",
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/post_tool_use_failure.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type PostToolUseFailure --summarize" }
        ]
      }
    ],
    "Notification": [
      {
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/notification.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type Notification --summarize" }
        ]
      }
    ],
    "PermissionRequest": [
      {
        "matcher": "",
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/permission_request.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type PermissionRequest --summarize" }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/session_start.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type SessionStart" }
        ]
      },
      {
        "matcher": "compact",
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/post_compact.py" }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/session_end.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type SessionEnd" }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/user_prompt_submit.py --log-only --store-last-prompt --name-agent" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type UserPromptSubmit --summarize" }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/stop.py --chat" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type Stop" }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/pre_compact.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type PreCompact" }
        ]
      }
    ],
    "SubagentStart": [
      {
        "matcher": "",
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/subagent_start.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type SubagentStart" }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          { "type": "command", "command": "uv run $HOME/.claude/hooks/subagent_stop.py" },
          { "type": "command", "command": "uv run $HOME/.claude/hooks/send_event.py --source-app cc-hook-multi-agent-obvs --event-type SubagentStop" }
        ]
      }
    ]
  }
}
